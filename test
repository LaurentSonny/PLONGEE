import java.awt.Font;
import java.awt.event.MouseEvent;

import edu.princeton.cs.introcs.StdDraw;

public class test {
	public final static int X_MAX = 10;
	public final static int Y_MAX = 10;

	public test() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StdDraw.setScale(-5, +5);
		StdDraw.filledRectangle(0, 0, 4, 3);
		StdDraw.setPenColor(StdDraw.GREEN);
		Font font = new Font("Arial", Font.BOLD, 10);
		StdDraw.setFont(font);
		StdDraw.text(0, 0, "HEY !");
		int keycode=0;
		StdDraw.isKeyPressed(keycode);
	}

	void mouseDown(MouseEvent e) {
		System.out.println(StdDraw.mouseX() + "/" + StdDraw.mouseY());
	}

}
